services:
  go:
    build:
      context: ./service
      dockerfile: Dockerfile
    image: holovision-goframe:1.0
    container_name: goframe-env
    restart: unless-stopped
    # ports:
    #   - 8000:8000
    volumes:
      - ./app:/app
    command: ["tail", "-f", "/dev/null"]
    networks:
      - goframe-net
      - ycsjnet
networks:
  goframe-net:
    name: goframe-net
    driver: bridge